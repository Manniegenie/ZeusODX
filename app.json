{
  "expo": {
    "name": "ZeusODX",
    "slug": "ZeusODX",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "zeusodx",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#35297F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.manniegenie.zeusodx",
      "icon": "./assets/images/app-icon.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "ZeusODX uses the camera to verify your identity.",
        "NSFaceIDUsageDescription": "Face ID may be used for identity verification."
      }
    },
    "android": {
      "package": "com.manniegenie.zeusodx",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#35297F"
      },
      "edgeToEdgeEnabled": true,
      "permissions": ["INTERNET", "CAMERA", "ACCESS_NETWORK_STATE"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/app-icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.24",
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 24
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "53868df3-1f0b-40e0-9c85-903a42b22c92"
      }
    },
    "owner": "manuelwurld",
    "runtimeVersion": { "policy": "appVersion" },
    "updates": {
      "url": "https://u.expo.dev/53868df3-1f0b-40e0-9c85-903a42b22c92"
    }
  }
}
