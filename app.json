{
  "expo": {
    "name": "ZeusODX",
    "slug": "zeusodx",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/app-icon.png",
    "scheme": "zeusodx",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#35297F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.manniegenie.zeusodx",
      "icon": "./assets/images/app-icon.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "ZeusODX uses the camera for identity verification and biometric authentication.",
        "NSFaceIDUsageDescription": "Face ID may be used for secure authentication.",
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "package": "com.manniegenie.zeusodx",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#35297F"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "pan",
      "windowSoftInputMode": "adjustResize",
      "resizeableActivity": true,
      "supportsTablet": true,
      "usesCleartextTraffic": false,
      "allowBackup": false,
      "hardwareAccelerated": true,
      "largeHeap": true,
      "permissions": [
        "INTERNET",
        "CAMERA",
        "ACCESS_NETWORK_STATE",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "com.google.android.c2dm.permission.RECEIVE",
        "android.permission.POST_NOTIFICATIONS"
      ],
      "versionCode": 30
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/app-icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24,
            "resizeableActivity": true,
            "supportsTablet": true,
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true,
            "enableR8": true,
            "enableHermes": true,
            "enableNewArchitecture": false
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/app-icon.png",
          "color": "#35297F",
          "sounds": [],
          "mode": "production"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7cc7ad65-2b13-42f3-9865-62b4e1d55984"
      }
    },
    "owner": "manuelwurld",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/7cc7ad65-2b13-42f3-9865-62b4e1d55984"
    },
    "sdkVersion": "53.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ]
  }
}
