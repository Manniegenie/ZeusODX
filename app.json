{
  "expo": {
    "name": "ZeusODX",
    "slug": "ZeusODX",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/zeus-logo.png",
    "scheme": "zeusodx",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.manniegenie.zeusodx"
    },
    "android": {
      "package": "com.manniegenie.zeusodx",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#35297F"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/app-icon.png"
    },
    "splash": {
      "image": "./assets/images/splashscreen_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#35297F"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "preventAutoHideAsync": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "53868df3-1f0b-40e0-9c85-903a42b22c92"
      }
    },
    "owner": "manuelwurld"
  }
}
